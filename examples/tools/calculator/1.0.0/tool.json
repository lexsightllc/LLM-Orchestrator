{
  "name": "calculator",
  "version": "1.0.0",
  "aliases": ["latest", "stable"],
  "description": "A simple calculator tool",
  "input_schema": {
    "type": "object",
    "properties": {
      "operation": {
        "type": "string",
        "enum": ["add", "subtract", "multiply", "divide"]
      },
      "a": {"type": "number"},
      "b": {"type": "number"}
    },
    "required": ["operation", "a", "b"]
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "result": {"type": "number"},
      "operation": {"type": "string"}
    },
    "required": ["result", "operation"]
  },
  "migrations": {
    "1.0.0->1.1.0": "migrations/1.0.0_to_1.1.0.py"
  }
}
